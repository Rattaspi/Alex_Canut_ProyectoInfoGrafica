#version 330 core

// Input vertex data, different for all executions of this shader.
layout(location = 0) in vec3 pos;
layout(location = 1) in vec3 col;
layout(location = 2) in vec2 tex;

uniform mat4 transf;

out vec2 TexCoord;

void main(){
	gl_Position = transf * (vec4(-1*pos,1));
	TexCoord = tex;
}